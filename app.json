{
  "expo": {
    "name": "Mebious",
    "slug": "Mebious",
    "privacy": "unlisted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mebious",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121113"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cleopatra.Mebious",
      "buildNumber": "0.0.1"
    },
    "android": {
      "package": "com.cleopatra.Mebious",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#121113"
      }
    },
    "androidNavigationBar": {
      "visible": "immersive"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "./plugins/withAndroidStyle.js",
      "./plugins/withDrawableAssets.js"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c7c55373-ccd4-42c7-9466-9cc22db4c66a"
      }
    }
  }
}
